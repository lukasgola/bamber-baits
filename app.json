    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shloyder.bamberbaits"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.shloyder.bamberbaits"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
