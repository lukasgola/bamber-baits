{
  "expo": {
    "name": "Bamber Baits (Daro)",
    "slug": "bamber-baits",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon_ios.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
